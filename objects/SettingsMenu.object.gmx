        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Activate options
if(menudepth == instance_number(_menu)) {
    if(input_check_pressed(ACTION)) {
        switch(curOpt) {
            case 1: //Music
                Controller.music = !Controller.music;
                break;
            case 2: //Sound
                Controller.sound = !Controller.sound;
                break;
            case 3: //Text speed
                Controller.DIASPDmult = (Controller.DIASPDmult % 3) + 1;
                break;
            case 4: //Controls
                switch(Controller.controls) {
                    case "^&lt;v&gt;ZXC":
                        Controller.controls = "WASD QE";
                        break;
                    case "WASD QE":
                        Controller.controls = "^&lt;v&gt;WXC";
                        break;
                    case "^&lt;v&gt;WXC":
                        Controller.controls = "ZQSD AE";
                        break;
                    case "ZQSD AE":
                    default:
                        Controller.controls = "^&lt;v&gt;ZXC";
                        break;
                }
                break;
            case 0: //Back
            default: 
                instance_destroy();
                break;
        }
        input_clear(ACTION);
    }
}
</string>
          </argument>
        </arguments>
      </action>
