        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Draw menu with values
if(menudepth == instance_number(_menu)) {
    draw_set_alpha(1);
    var value;
    switch(Controller.controls) {
        case "^&lt;v&gt;ZXC":
            value[4] = "Arrows, Z, X, C";
            break;
        case "^&lt;v&gt;WXC":
            value[4] = "Arrows, W, X, C";
            break;
        case "WASD QE":
            value[4] = "WASD, space, Q, E";
            break;
        case "ZQSD AE":
            value[4] = "ZQSD, space, A, E";
            break;
    }
    value[3] = string(Controller.DIASPDmult * room_speed * DIASPD) + " letters per second";
    if(Controller.sound) {
        value[2] = "On";
    } else {
        value[2] = "Off";
    }
    if(Controller.music) {
        value[1] = "On";
    } else {
        value[1] = "Off";
    }
    value[0] = "";
    draw_set_halign(fa_center);
    draw_set_valign(fa_bottom);
    draw_set_font(fMenu);
    if(os_browser != browser_not_a_browser) draw_set_font(fMenuHTML5);
    for(var i = 0; i &lt; array_length_1d(opt); i++) {
        if(i == curOpt) {
            draw_sprite(    sprite_index,
                            image_index,
                            window_get_width() / 2 - string_width(opt[i] + value[i]) / 2 - 40,
                            window_get_height() / 2 - (array_length_1d(opt) / 2) * 32 + i * 32);
            draw_sprite(    sprite_index,
                            image_index + 2,
                            window_get_width() / 2 + string_width(opt[i] + value[i]) / 2 + 8,
                            window_get_height() / 2 - (array_length_1d(opt) / 2) * 32 + i * 32);
        }
        draw_text(  window_get_width() / 2,
                    window_get_height() / 2 - (array_length_1d(opt) / 2) * 32 + (i + 1) * 32,
                    opt[i] + value[i]);
    }
    draw_set_halign(fa_left);
    draw_set_valign(fa_top);
}
</string>
